{
    "name": "lamoda/atol-client-bundle",
    "description": "Symfony Bundle for integration with lamoda/atol-client",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Agaletskiy",
            "email": "pavel.agaletskiy@lamoda.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "lamoda/atol-client": "^1.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/yaml": "^3.4 || ^4.0",
        "jms/serializer-bundle": "^2.3",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "symfony/symfony": "~2.8 || ~3.4 || ~4.0",
        "phpunit/phpunit": "^5.7.10",
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "Lamoda\\AtolClientBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lamoda\\AtolClientBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.1"
        }
    }
}
